{
  "schemaVersion": 1,
  "id": "emi_tree2quest",
  "version": "${version}",
  "name": "EMI Tree2Quest",
  "description": "A mod which can export EMI's Recipe Tree to FTB Quest!",
  "authors": [
    "zeutd"
  ],
  "contact": {
    "homepage": "https://fabricmc.net/",
    "sources": "https://github.com/FabricMC/fabric-example-mod"
  },
  "license": "CC0-1.0",
  "icon": "assets/emi_tree2quest/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "com.zeutd.tree2quest.fabric.Tree2QuestModFabric"
    ],
    "client": [
      "com.zeutd.tree2quest.fabric.client.Tree2QuestModFabricClient"
    ]
  },
  "mixins": [
    "emi_tree2quest.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.18.4",
    "minecraft": "~1.20.1",
    "java": ">=17",
    "fabric-api": "*",
    "emi": ">=${emi_version}",
    "ftbquests": ">=${ftb_quests_version}"
  },
  "suggests": {
    "another-mod": "*"
  }
